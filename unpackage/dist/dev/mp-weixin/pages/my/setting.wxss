
view,
label,
swiper-item,
scroll-view {
  display:flex;
  flex-direction:column;
  flex-shrink: 0;
  flex-grow: 0;
  flex-basis: auto;
  align-items: stretch;
  align-content: flex-start;
}
view,
image,
input,
scroll-view,
swiper,
swiper-item,
text,
textarea,
video {
  position: relative;
  border: 0px solid #000000;
  box-sizing: border-box;
}
swiper-item {
  position: absolute;
}
button {
  margin: 0;
}


.set-line{
	display: flex; 
	flex-direction: row; 
	height: 6vh; 
	background-color: white; 
	margin-top: 0.2vh;
}
.set-line-hover{
	background-color: aliceblue;
}
.ToLogin{
	text-align: center; 
	line-height: 6vh; 
	width: 100%; 
	margin-top: 0.8vh; 
	background-color: white;
}
.icons{
	width: 10%;
	margin-left: 2.5%;
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
}
.line-text{
	line-height: 6vh;
	width: 70%;
	margin-left: 2.5%;
	font-size: 1.8vh;
}
.line-image{
	width: 20%;
	margin-right: 5%;
	height: 6vh;
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
}

